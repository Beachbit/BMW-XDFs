<customexpressions>
	<customexpression>
		<ecu>BMS46DS0</ecu>
		<job>STATUS_INT</job>
		<result>
			<resultname>STAT_MY_RESULT</resultname>
			<expression>STATUS_INT_WERT*5</expression>	
		</result>
		<result>
			<resultname>STAT_MY_RESULT2</resultname>
			<expression>STATUS_INT_WERT*10</expression>	
		</result>
	</customexpression>
	<customexpression>
		<ecu>D73N57C0</ecu>		
		<job>HP</job>
		<result>
			<resultname>STAT_HorsePower_1</resultname>
			<expression>1*STAT_FAHRZEUGBESCHLEUNIGUNG_WERT*STAT_GESCHWINDIGKEIT_WERT</expression>
		</result>
		<result>
			<resultname>STAT_HorsePower_2</resultname>
			<expression>2*STAT_MoFVSS_a_WERT*STAT_MoFVSS_vAccMon_WERT</expression>
		</result>
		<result>
			<resultname>STAT_HorsePower_3</resultname>
			<expression>3*STAT_FAHRZEUGBESCHLEUNIGUNG_WERT*STAT_MoFVSS_vAccMon_WERT</expression>
		</result>
	</customexpression>
	<customexpression>
		<ecu>D73N57C0</ecu>		
		<job>SET_1</job>
		<result>
			<resultname>STAT_MY_RESULT</resultname>
                        <expression>STAT_AirCtl_rDesVal_WERT*1</expression>
		</result>
		<result>
			<resultname>STAT_MY_RESULT2</resultname>
                        <expression>STAT_AirCtl_rDesVal_WERT*2</expression>
		</result>
		<result>
			<resultname>STAT_MY_RESULT3</resultname>
                        <expression>STAT_AirCtl_rDesVal_WERT*3</expression>
		</result>
	</customexpression>
	
	<customexpression>
		<ecu>DM338DS1</ecu>
		<job>STATUS_INT_2</job>
		<result>
			<resultname>STAT_MY_RESULT</resultname>
			<expression>STATUS_LMM_WERT*10</expression>	
		</result>
		<result>
			<resultname>STAT_MY_RESULT2</resultname>
			<expression>STATUS_LMM_WERT/4</expression>	
		</result>
	</customexpression>
	<customexpression>
         	<ecu>MSS54DS0_SERIAL</ecu>
         	<job>STATUS_MOTOR_1</job>
         	<result>
             		<resultname>Exhaust_Temp_degF</resultname>
             		<expression>TABG*(9/5)+32</expression>    
         	</result>
         	<result>
             		<resultname>Intake_Air_Temp_degF</resultname>
             		<expression>AN_LUFTTEMPERATUR*(9/5)+32</expression>    
         	</result>
		<result>
             		<resultname>Coolant_Temp_degF</resultname>
             		<expression>MOTORTEMPERATUR*(9/5)+32</expression>    
         	</result>
		<result>
             		<resultname>Radiator_Out_Temp_degF</resultname>
             		<expression>KUEHLW_AUSL_TEMPERATUR*(9/5)+32</expression>    
         	</result>
		<result>
             		<resultname>Oil_Temp_degF</resultname>
             		<expression>OEL_TEMPERATUR*(9/5)+32</expression>    
         	</result>
		<result>
             		<resultname>Exterior_Temp_degF</resultname>
             		<expression>TUMG*(9/5)+32</expression>    
         	</result>
         	<job>STATUS_MOTOR_2</job>
         	<result>
             		<resultname>Speed_MPH</resultname>
             		<expression>GESCHWINDIGKEIT/1.6</expression>    
         	</result>
     	</customexpression>
   
</customexpressions>

